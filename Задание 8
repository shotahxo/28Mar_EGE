k=0
for x1 in '12345678':
	for x2 in '012345678':
		for x3 in '012345678':
			for x4 in '012345678':
				for x5 in '012345678':
					for x6 in '012345678':
						for x7 in '012345678':
							x=x1+x2+x3+x4+x5+x6+x7
							if x[0] not in '1357':
								if x[-1] not in '036':
									if x.count('6')>=1:
										k+=1
print(k)
									
# Ответ: 827352